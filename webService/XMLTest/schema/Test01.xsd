<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
        targetNamespace="http://www.example.org/Test01"
        xmlns:tns="http://www.example.org/Test01" 
        elementFormDefault="qualified">
